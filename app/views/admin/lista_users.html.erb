<div class="container-sm py-5 my-5">
  <table class="table table-dark table-hover">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Name:</th>
        <th scope="col">CPF:</th>
        <th scope="col">Admin:</th>
      </tr>
    </thead>
    <tbody>
      <% @lista_users.each do |user| %>
        <tr>
          <th scope="row"><%= user.id %></th>
          <td><%= user.nome %></td>
          <td><%= user.cpf %></td>
          <td><%= user.admin.name %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

</div>
